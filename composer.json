{
  "name": "guysartorelli/ss-dev-kit",
  "description": "Dev tools CLI for new Silverstripe developers",
  "license": "BSD-3-Clause",
  "bin": [
    "bin/ss-dev-kit"
  ],
  "autoload": {
    "psr-4": {
      "Silverstripe\\DevKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Silverstripe\\DevKit\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "symfony/console": "^6.0",
    "symfony/process": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/filesystem": "^6.0",
    "composer/semver": "^3.3",
    "twig/twig": "^3.4",
    "gitonomy/gitlib": "^1.3",
    "icanhazstring/symfony-console-spinner": "^1.1",
    "symfony/yaml": "^6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpunit/phpunit": "^10.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
